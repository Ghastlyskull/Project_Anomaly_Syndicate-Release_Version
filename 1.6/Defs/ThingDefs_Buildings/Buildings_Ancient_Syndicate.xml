    <!-- Ancient Obelisk Suppresor -->
    <ThingDef ParentName="NonDeconstructibleAncientBuildingBase">
        <defName>AncientObeliskSuppresor</defName>
        <label>ancient obelisk suppresor</label>
        <description>An ancient, broken obelisk suppresor. Everything that isn't rusted away was looted long ago.</description>
        <graphicData>
          <texPath>Things/Buildings/Ruins/Obelisk_Suppresor_Ancient</texPath>
          <graphicClass>Graphic_Single</graphicClass>
          <drawSize>(2,2)</drawSize>
        </graphicData>
        <size>(2,2)</size>
        <fillPercent>0.1</fillPercent>
        <passability>PassThroughOnly</passability>
